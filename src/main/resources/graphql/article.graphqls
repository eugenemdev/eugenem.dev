schema {
        query: QueryType
    }
type QueryType {
  lastArticlesList: [Article]
  getName(name : String) : String
  getArticleById(id: Long) : Article

    }

type Specification {
  specification: String
}
type Article {
        id: Long,
        date: String,
        title: String,    
        template: String,
        thumbnail: String,
        slug: String,
        categories: String,
        tags: String,
        fileName: String,
        body: [Specification]
    }

